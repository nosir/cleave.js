{
  "stripDelimiters": [
    {
      "params": [
        "12-3",
        "-",
        []
      ],
      "expected": "123"
    },
    {
      "params": [
        "1203",
        "0",
        []
      ],
      "expected": "123"
    },
    {
      "params": [
        "1273",
        "7",
        []
      ],
      "expected": "123"
    },
    {
      "params": [
        "12-45 78|0",
        "",
        [
          "-",
          " ",
          "|"
        ]
      ],
      "expected": "1245780"
    },
    {
      "params": [
        "123456789",
        "",
        [
          "3",
          "5",
          "7",
          "9"
        ]
      ],
      "expected": "12468"
    }
  ],
  "getFormattedValue": [
    {
      "params": [
        "123",
        [3, 3],
        2,
        "-",
        [],
        false
      ],
      "expected": "123-"
    },
    {
      "params": [
        "123456",
        [3, 3],
        2,
        "-",
        [],
        false
      ],
      "expected": "123-456"
    },
    {
      "params": [
        "123",
        [3, 3],
        2,
        "-",
        [],
        true
      ],
      "expected": "123"
    },
    {
      "params": [
        "12345",
        [3, 3],
        2,
        "-",
        [],
        true
      ],
      "expected": "123-45"
    },
    {
      "params": [
        "123456",
        [3, 3, 3],
        3,
        "",
        ["-", "X"],
        true
      ],
      "expected": "123-456"
    },
    {
      "params": [
        "123456789",
        [3, 3, 3],
        3,
        "",
        ["-", "X"],
        true
      ],
      "expected": "123-456X789"
    }
  ],
  "getPrefixStrippedValue": [
    {
      "params": [
        "tst123",
        "test",
        4,
        "test123"
      ],
      "expected": "123"
    },
    {
      "params": [
        "teast",
        "test",
        4,
        "test"
      ],
      "expected": "a"
    }
  ]
}
